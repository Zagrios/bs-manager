    "engines": {
        "node": ">=20.0.0"
    },
    "collective": {
        "url": "https://www.patreon.com/bsmanager"
    },
    "browserslist": [],
    "prettier": {
        "printWidth": 1000,
        "bracketSameLine": false,
        "arrowParens": "avoid",
        "semi": true,
        "singleQuote": false,
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "tabWidth": 4,
        "overrides": [
            {
                "files": [
                    ".prettierrc",
                    ".eslintrc"
                ],
                "options": {
                    "parser": "json"
                }
            }
        ]
    },
    "electronmon": {
        "patterns": [
            "!src/__tests__/**",
            "!release/**",
            "!assets/**"
        ],
        "logLevel": "quiet"
    },
    "volta": {
        "node": "20.11.0"
    }
}
